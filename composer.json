{
    "name": "ac/web-services-bundle",
    "type": "symfony-bundle",
    "description": "Provides tools for developing RESTful APIs.",
    "keywords": ["rest", "web services"],
    "license": "MIT",
    "authors": [
        {
            "name": "Evan Villemez",
            "email": "evan.villemez@americancouncils.org"
        }
    ],
    "target-dir": "AC/WebServicesBundle",
    "autoload": {
        "psr-0": { "AC\\WebServicesBundle": "" }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "symfony/framework-bundle": "~2.2",
        "jms/serializer-bundle": "~0.13",
        "willdurand/negotiation": "~1.2",
        "mockery/mockery": "~0.9",
        "fzaninotto/faker": "~1.3",
        "lstrojny/functional-php": "*@dev"
    },
    "require-dev": {
        "symfony/symfony": "~2.2",
        "symfony/monolog-bundle": "~2.2",
        "sensio/framework-extra-bundle": "~2.2",
        "phpunit/phpunit": "~4.0"
    },
    "conflict": {
        "jms/serializer": "<0.15",
        "jms/serializer-bundle": "<0.13"
    }
}
